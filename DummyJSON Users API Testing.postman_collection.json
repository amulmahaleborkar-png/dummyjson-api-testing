		{
			"name": "POST – Create User",
			"event": [
				{
					"listen": "test",
					"script": {
						"type": "text/javascript",
						"exec": [
							"// POST – Create User: Positive and Negative Test Cases",
							"var jsonData = pm.response.json();",
							"",
							"// Positive: Status code and structure",
							"pm.test(\"Status code is 200 or 201\", function () {",
							"    pm.expect(pm.response.code).to.be.oneOf([200, 201]);",
							"});",
							"pm.test(\"Response contains created user with required fields\", function () {",
							"    pm.expect(jsonData).to.be.an('object');",
							"    pm.expect(jsonData).to.have.property('id').that.is.a('number');",
							"    pm.expect(jsonData).to.have.property('firstName').that.is.a('string');",
							"    pm.expect(jsonData).to.have.property('lastName').that.is.a('string');",
							"    pm.expect(jsonData).to.have.property('email').that.is.a('string');",
							"});",
							"",
							"// Negative: Missing required field",
							"pm.sendRequest({",
							"    url: pm.request.url.toString(),",
							"    method: 'POST',",
							"    header: {'Content-Type': 'application/json'},",
							"    body: { mode: 'raw', raw: JSON.stringify({ lastName: 'Doe', email: 'jane@example.com' }) }",
							"}, function (err, res) {",
							"    pm.test(\"Missing firstName returns 400 or error\", function () {",
							"        pm.expect([400, 422, 500]).to.include(res.code);",
							"        pm.expect(res.json().message || res.json().error).to.match(/firstName|required/i);",
							"    });",
							"});",
							"",
							"// Negative: Unauthorized (simulate by sending to a protected endpoint)",
							"pm.sendRequest({",
							"    url: pm.request.url.toString().replace('/add', '/add/protected'),",
							"    method: 'POST',",
							"    header: {'Content-Type': 'application/json'},",
							"    body: { mode: 'raw', raw: JSON.stringify({ firstName: 'Jane', lastName: 'Doe', email: 'jane@example.com' }) }",
							"}, function (err, res) {",
							"    pm.test(\"Unauthorized returns 401 or 403\", function () {",
							"        pm.expect([401, 403, 404]).to.include(res.code);",
							"    });",
							"});"
						]
					}
				}
			],
